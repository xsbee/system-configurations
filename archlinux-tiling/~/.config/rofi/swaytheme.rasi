* {
	background-color: transparent;
	text-color: #ffffff;
}

window {
	border: 2px;
	border-color: #4c7899;
	background-color: #222222;
}

inputbar {
	padding: 8px 8px;
	spacing: 8px;
	background-color: #285577;
}

prompt {
	text-color: #cccccc;
}

element {
	padding: 4px;
}

element urgent {
	background-color: #900000;	
}

element selected {
	background-color: #396688;
}

element-icon {
	size: 1.2em;
}

